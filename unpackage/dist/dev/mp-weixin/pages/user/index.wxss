
.order-view {
	width: 96%;

	display: -webkit-box;

	display: -webkit-flex;

	display: flex;
	-webkit-justify-content: space-around;
	        justify-content: space-around;
	-webkit-box-orient: horizontal;
	-webkit-box-direction: normal;
	-webkit-flex-direction: row;
	        flex-direction: row;
	background-color: #ffffff;
	height: 160rpx;

	/* 		border-top-left-radius: 40upx;
	border-top-right-radius: 40upx; */
	padding-left: 30rpx;
}
.order-view-image {
	width: 50rpx;
	height: 50rpx;
}
.order-view-view {
	text-align: center;
	padding: 40rpx 30rpx 40rpx 10rpx;
	/* width: 180upx; */
}
.order-view-text {
	color: #000000;
	font-size: 12px;
	margin-top: 10rpx;
	margin-left: 8rpx;
}
.vip-card-box {

	display: -webkit-box;

	display: -webkit-flex;

	display: flex;
	-webkit-box-orient: vertical;
	-webkit-box-direction: normal;
	-webkit-flex-direction: column;
	        flex-direction: column;
	color: #f7d680;
	background: url(data:image/png;base64,);
	background-size: 100% 100%;
	border-radius: 16rpx 16rpx 0 0;
	overflow: hidden;
	position: absolute;
	left: 0;
	top: 250rpx;
	width: 96%;
	height: 100rpx;
	padding: 20rpx 24rpx;
	margin-left: 2vw;
}
.b-btn {
	position: absolute;
	right: 20rpx;
	top: 20rpx;
	width: 132rpx;
	height: 40rpx;
	text-align: center;
	line-height: 40rpx;
	font-size: 22rpx;
	color: #36343c;
	border-radius: 20px;
	background: -webkit-linear-gradient(left, #f9e6af, #ffd465);
	background: linear-gradient(left, #f9e6af, #ffd465);
	z-index: 1;
}
.tit {
	font-size: $font-base+2upx;
	color: #f7d680;
	margin-bottom: 28rpx;
}
.arc {
	position: absolute;
	left: 0;
	top: 320rpx;
	width: 100%;
	height: 36rpx;
}
.yticon {
	color: #f6e5a3;
	margin: 16rpx;
}
.e-b {
	font-size: $font-sm;
	color: #d8cba9;
	margin-top: 10rpx;
}
